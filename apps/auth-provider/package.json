{
  "name": "f3-nation-auth-provider",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run '**/*.unit.test.ts'",
    "test:integration": "vitest run '**/*.integration.test.ts'",
    "test:db": "vitest run '**/*.db.test.ts'",
    "test:coverage": "vitest run --coverage",
    "db:deploy": "npx tsx scripts/db/migrate.ts",
    "firebase:secrets": "scripts/firebase-secrets.sh all",
    "firebase:secrets:prod": "scripts/firebase-secrets.sh prod",
    "firebase:secrets:staging": "scripts/firebase-secrets.sh staging",
    "prune:secrets": "scripts/prune-old-secrets.sh",
    "prune:secrets:dry-run": "scripts/prune-old-secrets.sh --dry-run",
    "db:snapshot": "scripts/db/snapshot.sh",
    "db:local:up": "scripts/db/local/up.sh",
    "db:local:down": "scripts/db/local/down.sh",
    "db:local:seed": "scripts/db/local/seed.sh",
    "db:local:reset": "scripts/db/local/reset.sh"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4",
    "base64-url": "^2.3.3",
    "next": "15.4.10",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pg": "^8.11.0",
    "react": "19.1.4",
    "react-dom": "19.1.4",
    "tailwindcss": "^4"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.18.0",
    "@testing-library/react": "^16.0.0",
    "@types/base64-url": "^2.2.2",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "msw": "^2.7.0",
    "prettier": "^3.6.2",
    "testcontainers": "^10.18.0",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
